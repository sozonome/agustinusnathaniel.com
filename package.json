{
	"name": "agustinusnathaniel.com",
	"version": "5.0.0",
	"author": "sozonome",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"release": "cross-env HUSKY=0 standard-version",
		"pre-release": "yarn release -- --prerelease",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.0.3",
		"@commitlint/cz-commitlint": "^17.0.3",
		"@iconify-json/fa-brands": "^1.1.2",
		"@iconify-json/twemoji": "^1.1.5",
		"@notion-stuff/v4-types": "^6.0.0",
		"@sveltejs/adapter-auto": "1.0.0-next.67",
		"@sveltejs/kit": "1.0.0-next.445",
		"@types/lodash-es": "^4.17.6",
		"@types/umami": "^0.1.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"commitizen": "^4.2.5",
		"commitlint": "^17.0.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^12.5.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"standard-version": "^9.5.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"unocss": "0.45.13",
		"vite": "3.0.9"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.8",
		"@notionhq/client": "^1.0.4",
		"clsx": "^1.2.1",
		"lodash-es": "^4.17.21",
		"notion-to-md": "^2.5.3",
		"svelte-markdown": "^0.2.2",
		"svelte-meta-tags": "^2.6.1"
	},
	"resolutions": {
		"@sveltejs/vite-plugin-svelte": "1.0.0-next.48"
	}
}
