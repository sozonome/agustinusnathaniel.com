{
	"name": "agustinusnathaniel.com",
	"version": "5.1.3",
	"author": "sozonome",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build": "vite build",
		"build:turbo": "npx turbo build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:fix": "prettier --check --plugin-search-dir=. . && eslint --fix --ignore-path .gitignore .",
		"lint:turbo": "npx turbo lint",
		"format": "prettier --write --plugin-search-dir=. .",
		"release": "cross-env HUSKY=0 standard-version",
		"pre-release": "pnpm release -- --prerelease",
		"up-interactive": "pnpm up -i",
		"up-latest": "pnpm up-interactive -L",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.4.4",
		"@commitlint/cz-commitlint": "^17.5.0",
		"@commitlint/types": "^17.4.4",
		"@iconify-json/fa-brands": "^1.1.4",
		"@iconify-json/twemoji": "^1.1.11",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "^1.15.4",
		"@types/lodash-es": "^4.17.7",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"@unocss/extractor-svelte": "^0.52.0",
		"@unocss/preset-icons": "^0.52.0",
		"@unocss/preset-uno": "^0.52.0",
		"@unocss/reset": "^0.52.0",
		"commitizen": "^4.3.0",
		"commitlint": "^17.5.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-sznm": "^1.1.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"standard-version": "^9.5.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"unocss": "0.51.4",
		"vite": "4.2.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.11",
		"@notionhq/client": "^2.2.4",
		"clsx": "^1.2.1",
		"lodash-es": "^4.17.21",
		"notion-to-md": "^2.5.5",
		"svelte-markdown": "^0.2.3",
		"svelte-meta-tags": "^2.7.0"
	},
	"engines": {
		"node": ">=16.8.x",
		"pnpm": ">=8"
	},
	"packageManager": "pnpm@8.1.0"
}
