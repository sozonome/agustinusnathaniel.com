{
	"name": "agustinusnathaniel.com",
	"version": "5.0.0",
	"author": "sozonome",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"release": "standard-version",
		"pre-release": "yarn release -- --prerelease",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.0.0",
		"@iconify-json/fa-brands": "^1.1.1",
		"@iconify-json/twemoji": "^1.1.4",
		"@notion-stuff/v4-types": "^5.1.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.42",
		"@sveltejs/kit": "^1.0.0-next.330",
		"@types/lodash-es": "^4.17.6",
		"@types/umami": "^0.1.0",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"commitlint": "^17.0.0",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^12.4.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"standard-version": "^9.5.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4",
		"unocss": "^0.33.5"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.8",
		"@notionhq/client": "^0.4.13",
		"clsx": "^1.1.1",
		"lodash-es": "^4.17.21",
		"notion-to-md": "^2.3.3",
		"svelte-markdown": "^0.2.2",
		"svelte-meta-tags": "^2.6.0"
	}
}
