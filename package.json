{
	"name": "agustinusnathaniel.com",
	"version": "5.1.4",
	"author": "sozonome",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"turbo": "pnpm dlx turbo@2.0.9",
		"build": "vite build",
		"build:turbo": "pnpm turbo build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check src && eslint --ignore-path .gitignore .",
		"lint:fix": "prettier --check src && eslint --fix --ignore-path .gitignore .",
		"lint:turbo": "pnpm turbo lint",
		"format": "prettier --write src",
		"release": "cross-env HUSKY=0 standard-version",
		"pre-release": "pnpm release -- --prerelease",
		"up-interactive": "pnpm up -i",
		"up-latest": "pnpm up-interactive -L",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.5.0",
		"@commitlint/cz-commitlint": "^19.5.0",
		"@commitlint/types": "^19.5.0",
		"@iconify-json/fa-brands": "^1.2.0",
		"@iconify-json/twemoji": "^1.2.1",
		"@sveltejs/adapter-vercel": "^5.4.6",
		"@sveltejs/kit": "^2.7.4",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/lodash-es": "^4.17.12",
		"@typescript-eslint/eslint-plugin": "^8.12.2",
		"@typescript-eslint/parser": "^8.12.2",
		"@unocss/extractor-svelte": "^0.63.6",
		"@unocss/reset": "^0.63.6",
		"commitizen": "^4.3.1",
		"commitlint": "^19.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"standard-version": "^9.5.0",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"unocss": "0.63.6",
		"vite": "5.4.10"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^5.1.0",
		"@notionhq/client": "^2.2.15",
