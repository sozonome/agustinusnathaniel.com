{
	"name": "agustinusnathaniel.com",
	"version": "5.0.0-alpha",
	"author": "sozonome",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"release": "standard-version",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"up-next": "yarn up --next",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^16.0.0",
		"@iconify-json/twemoji": "^1.0.2",
		"@notion-stuff/v4-types": "^5.1.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.7",
		"@sveltejs/kit": "^1.0.0-next.212",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"commitlint": "^16.0.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"husky": "^7.0.0",
		"lint-staged": "^12.1.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"standard-version": "^9.3.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"unocss": "^0.20.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.1",
		"@notionhq/client": "^0.4.11",
		"notion-to-md": "^2.1.0",
		"svelte-markdown": "^0.2.1",
		"svelte-meta-tags": "^2.2.2"
	}
}
