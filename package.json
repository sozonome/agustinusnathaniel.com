{
	"name": "agustinusnathaniel.com",
	"version": "5.1.3",
	"author": "sozonome",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:turbo": "npx turbo build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:fix": "prettier --check --plugin-search-dir=. . && eslint --fix --ignore-path .gitignore .",
		"lint:turbo": "npx turbo lint",
		"format": "prettier --write --plugin-search-dir=. .",
		"release": "cross-env HUSKY=0 standard-version",
		"pre-release": "yarn release -- --prerelease",
		"up": "yarn upgrade-interactive",
		"up-latest": "yarn up --latest",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.4.4",
		"@commitlint/cz-commitlint": "^17.4.4",
		"@iconify-json/fa-brands": "^1.1.4",
		"@iconify-json/twemoji": "^1.1.11",
		"@notion-stuff/v4-types": "^6.0.0",
		"@sveltejs/adapter-auto": "2.0.0",
		"@sveltejs/kit": "^1.11.0",
		"@types/lodash-es": "^4.17.6",
		"@types/umami": "^0.1.0",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"commitizen": "^4.3.0",
		"commitlint": "^17.4.4",
		"cross-env": "^7.0.3",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-config-sznm": "^1.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"standard-version": "^9.5.0",
		"svelte": "^3.56.0",
		"svelte-check": "^3.1.2",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"turbo": "^1.8.3",
		"typescript": "^4.9.5",
		"unocss": "0.49.4",
		"vite": "4.1.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^4.5.11",
		"@notionhq/client": "^1.0.4",
		"clsx": "^1.2.1",
		"lodash-es": "^4.17.21",
		"notion-to-md": "^2.5.5",
		"svelte-markdown": "^0.2.3",
		"svelte-meta-tags": "^2.7.0"
	}
}
