{
	"name": "agustinusnathaniel.com",
	"version": "5.1.4",
	"author": "sozonome",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build": "vite build",
		"build:turbo": "npx turbo build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check src && eslint --ignore-path .gitignore .",
		"lint:fix": "prettier --check src && eslint --fix --ignore-path .gitignore .",
		"lint:turbo": "npx turbo lint",
		"format": "prettier --write src",
		"release": "cross-env HUSKY=0 standard-version",
		"pre-release": "pnpm release -- --prerelease",
		"up-interactive": "pnpm up -i",
		"up-latest": "pnpm up-interactive -L",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.2.2",
		"@commitlint/cz-commitlint": "^19.2.0",
		"@commitlint/types": "^19.0.3",
		"@iconify-json/fa-brands": "^1.1.8",
		"@iconify-json/twemoji": "^1.1.15",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/kit": "^2.5.6",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/lodash-es": "^4.17.12",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"@unocss/extractor-svelte": "^0.59.4",
		"@unocss/reset": "^0.59.4",
		"commitizen": "^4.3.0",
		"commitlint": "^19.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-sznm": "^2.0.3",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"standard-version": "^9.5.0",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"svelte-preprocess": "^5.1.4",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unocss": "0.58.6",
		"vite": "5.1.6"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/outfit": "^5.0.12",
		"@notionhq/client": "^2.2.15",
		"clsx": "2.1.0",
		"lodash-es": "^4.17.21",
		"notion-to-md": "3.1.1",
		"svelte-markdown": "0.4.1",
		"svelte-meta-tags": "^3.1.2"
	},
	"engines": {
		"node": ">=16.8.x",
		"pnpm": ">=8"
	}
}